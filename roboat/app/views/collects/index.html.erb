<p id="notice"><%= notice %></p>

<h1>Collects</h1>

<table>
  <thead>
    <tr>
      <th>Ph</th>
      <th>Turbidity</th>
      <th>Temperature</th>
      <th>Conductivity</th>
      <th>Date</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @collects.each do |collect| %>
      <tr>
        <td><%= collect.ph %></td>
        <td><%= collect.turbidity %></td>
        <td><%= collect.temperature %></td>
        <td><%= collect.conductivity %></td>
        <td><%= collect.date %></td>
        <td><%= collect.location_id %></td>
        <td><%= link_to 'Show', collect %></td>
        <td><%= link_to 'Edit', edit_collect_path(collect) %></td>
        <td><%= link_to 'Destroy', collect, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Collect', new_collect_path %>
